<h1><%= current_user.name %>'s Profile</h1>
<p><%= current_user.address %></p>
<p><%= current_user.city %></p>
<p><%= current_user.state %></p>
<p><%= current_user.zip %></p>
<p><%= current_user.user_name %></p>


<% @orders.each do |order| %>
    <%= link_to "Order #{order.id}", merchant_order_show(order.id) %>
    <p>Date Created: <%= @order.created_at %></p>
    <p>Order Created: <%= @order.total_quantity_merchant %></p>
    <p>Order Created: <%= @order.total_value %></p>
<% end %>
